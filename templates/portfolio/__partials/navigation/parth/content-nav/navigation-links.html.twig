<ul>
    {%
    set navigationLists = {
    'presentation': ['Cliquez ici pour me découvrir', 'presentation', 'presentation logo', 'présentation'],
    'diplome': ['Cliquez ici pour voir mon diplôme', 'graduation', 'diplome logo', 'Diplôme'],
    'experience': ['Cliquez ici pour voir mes éxperiences', 'programming', 'experience logo', 'Expériences'],
    'langage': ['Cliquez ici pour voir mes langages', 'scientific', 'Informatique langage logo', 'Langages'],
    'credit': ['Cliquez ici pour voir les crédits', 'slack', 'credit logo', 'crédits'],
    }
    %}

    {% for navigationList, detail in navigationLists %}
    <li title="{{detail[0]}}" data-link="{{navigationList}}">
        <div class="content-presentation">
            <img src="{{ asset('assets/pictures/icon/' ~ detail[1] ~ '.png') }}" alt="{{detail[2]}}">
            <span>{{detail[3]}}</span>
        </div>
    </li>
    {% endfor %}
</ul>